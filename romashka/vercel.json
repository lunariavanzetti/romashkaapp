{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "SUPABASE_URL": "",
    "SUPABASE_KEY": "",
    "OPENAI_API_KEY": "",
    "PADDLE_VENDOR_ID": "",
    "PADDLE_API_KEY": ""
  }
} 